{
  "manifest_version": 2,

  "name": "generals.io helper",
  "description": "A useful extension for playing generals.io",  
  "version": "1.0.1",
  "icons": {
    "16": "obstacle16.png",
    "48": "obstacle48.png",
    "128": "obstacle.png"
  },
  "content_scripts": [
    {
      "matches": ["http://generals.io/*"],
      "js": ["script.js"]
    }
  ]
}